/* components/InquiryRegistrationButton/InquiryRegistrationButton.wxss */
.InquiryRegistrationButton-container{
  position: absolute;/*绝对定位对应index的relative*/
  bottom: 5vh;
  width: 100%;
}
.btn-container {
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;

}

.inq-btn,
.reg-btn {
  font-size: 2.5vh;
  border-radius: 2vh;
  letter-spacing: 2px;
  padding: 0rem 1rem;
}

.inq-btn {
  color: rgb(64, 128, 255);
}
.reg-btn{
  color: white;
  background-color: rgb(64,128,255);
}
.reg-btn:active{
  color: rgb(235, 234, 234);
  background-color: rgb(58, 116, 233);
}